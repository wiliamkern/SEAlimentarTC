<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SEAlimentarTC.View.FoodMenuHistoryList"
             Visual="Material"
             Title="Cardápios">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>

                <Label Text="Histórico" HorizontalTextAlignment="Start" FontSize="23" Margin="30,50,10,10" FontFamily="ArialRoundedMT"  FontAttributes="Bold" TextColor="#505050" />

                <Frame BackgroundColor="#E6E6E6" CornerRadius="30" Padding="20"  HasShadow="True" Margin="30,0,30,30" >
                    <StackLayout Padding="10,0,10,0" HorizontalOptions="FillAndExpand">
                        <Label TextColor="#FF2C7733" FontAttributes="Bold" HorizontalOptions="Start" Padding="10,0,10,10" FontFamily="ArialRoundedMT" FontSize="16">Observações</Label>

                    </StackLayout>
                </Frame>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>